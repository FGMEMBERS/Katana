<?xml version="1.0"?>

<!-- Picture by Jentron -->

<PropertyList>

  <path>volts.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.030 </red>
      <green> 0.015 </green>
      <blue>  0.005 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>systems/electrical/volts</property>
    <interpolation>
      <entry><ind> 16 </ind><dep>  0.0 </dep></entry>
      <entry><ind> 20 </ind><dep> 23.5 </dep></entry>
      <entry><ind> 24 </ind><dep> 46.0 </dep></entry>
      <entry><ind> 28 </ind><dep> 68.0 </dep></entry>
      <entry><ind> 32 </ind><dep> 91.0 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <center>
      <x-m>  0.001 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.010 </z-m>
    </center>
  </animation>

</PropertyList>
