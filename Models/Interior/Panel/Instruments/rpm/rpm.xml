<?xml version="1.0" encoding="UTF-8"?>

<!-- Picture by Jentron -->

<PropertyList>

  <path>rpm.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.030 </red>
      <green> 0.015 </green>
      <blue>  0.005 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation> 
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine[0]/rpm</property>
    <interpolation>
      <entry><ind>    0 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  500 </ind><dep>  43.0 </dep></entry>
      <entry><ind> 1000 </ind><dep>  85.5 </dep></entry>
      <entry><ind> 1500 </ind><dep> 126.0 </dep></entry>
      <entry><ind> 2000 </ind><dep> 165.5 </dep></entry>
      <entry><ind> 2500 </ind><dep> 204.0 </dep></entry>
      <entry><ind> 3000 </ind><dep> 242.0 </dep></entry>
    </interpolation>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
  </animation>

</PropertyList>
