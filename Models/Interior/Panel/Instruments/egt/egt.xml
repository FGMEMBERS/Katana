<?xml version="1.0" encoding="UTF-8"?>

<!-- Picture by Jentron -->

<PropertyList>

  <path>egt.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.030 </red>
      <green> 0.015 </green>
      <blue>  0.005 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine/egt-degf</property>
    <interpolation>
      <entry><ind>  250 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  450 </ind><dep>  38.0 </dep></entry>
      <entry><ind>  650 </ind><dep>  76.0 </dep></entry>
      <entry><ind>  850 </ind><dep> 112.0 </dep></entry>
      <entry><ind> 1050 </ind><dep> 148.0 </dep></entry>
      <entry><ind> 1250 </ind><dep> 186.0 </dep></entry>
      <entry><ind> 1450 </ind><dep> 224.5 </dep></entry>
      <entry><ind> 1650 </ind><dep> 264.5 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
  </animation>

</PropertyList>
